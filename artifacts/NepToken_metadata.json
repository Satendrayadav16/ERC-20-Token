		"devdoc": {
			"kind": "dev",
			"methods": {
				"_burnToken(uint256)": {
					"details": "destroy amount of token fron owner account burned amount reduces value from `totalSupply`. requirements:- 1. owner account cannot be zero address   2. owner account value cannot be zero and have atleast some amount of token."
				},
				"_mintToken(uint256)": {
					"details": "generates amount token in owner account. adds to the `totalSupply` inncreasing the token amount. requirement:- cannot be zero address"
				},
				"allowance(address,address)": {
					"details": "returns remaining number of token that sender will be allowed to sent on behalf of owner. value zero by default value changes when `approve` or `transferFrom` function are called."
				},
				"approve(address,uint256)": {
					"details": "sets amount as allowance of sender over callers token. returns value as indicaiton of successful operation."
				},
				"balanceOf(address)": {
					"details": "returns the amount of token owned by the account"
				},
				"decimals()": {
					"details": "sets the assigned decimal after the value"
				},
				"name()": {
					"details": "sets the name of the token"
				},
				"symbol()": {
					"details": "sets the symbol of the token"
				},
				"totalSupply()": {
					"details": "sets the total supply of the token returns the total supply of token in existence"
				},
				"transfer(address,uint256)": {
					"details": "sends amount of token to receipent account from sender account. returns value indicating if operation is suceeded."
				},
				"transferFrom(address,address,uint256)": {
					"details": "moves amount of token from the sender to receipent account amount is then deducted from sender account after transfer is confirmed"
				}
			},
